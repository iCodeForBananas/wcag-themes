  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#586e75",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#eee8d5"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#b58900"
      }
