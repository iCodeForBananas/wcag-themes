  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#9485B3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#C77DFF"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#7FE7D4"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#E5D9FF"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF99CC"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#5BB8FF"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#B197FC"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#B197FC"
      }
