  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#85B3C9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#5FD4E6"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#7FE7D4"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#D9F0F7"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFB380"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#5BB8FF"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#7FE7F7"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#7FE7F7"
      }
    }
  ]
}
