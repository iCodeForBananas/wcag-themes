  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#616e88",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#81a1c1"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#a3be8c"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#e5e9f0"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b48ead"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#88c0d0"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#8fbcbb"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#8fbcbb"
      }
    }
  ]
}
