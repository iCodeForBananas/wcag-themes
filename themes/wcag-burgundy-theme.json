  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#B38593",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E67399"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#A3D977"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F5D9E3"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFB380"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#85C5FF"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#FF99CC"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#FF99CC"
      }
    }
