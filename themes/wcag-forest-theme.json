  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#85B393",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#5FD38D"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#7FE7A8"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#D9F5E3"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFB380"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#5BB8FF"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#A7F3D0"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#A7F3D0"
      }
    }
