    "focusBorder": "#B0B0B0",
    "input.background": "#141414",
    "input.foreground": "#F5F5F5",
    "input.border": "#909090",
    "inputOption.activeBorder": "#B0B0B0",
    "list.activeSelectionBackground": "#404040",
    "list.activeSelectionForeground": "#F5F5F5",
    "list.hoverBackground": "#1F1F1F",
    "list.hoverForeground": "#F5F5F5",
    "list.focusOutline": "#B0B0B0",
    "button.background": "#B0B0B0",
    "button.foreground": "#0A0A0A",
    "button.hoverBackground": "#C8C8C8"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#787878",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E8E8E8"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#EEEEEE"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#E0E0E0"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#E0E0E0"
      }
    }
  ]
}
