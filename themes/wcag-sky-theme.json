{
  "name": "WCAG Sky Theme",
  "type": "light",
  "colors": {
    "window.activeBorder": "#c8dce8",
    "editor.background": "#F0F8FF",
    "editor.foreground": "#0A1420",
    "editor.selectionBackground": "#B3D7FF",
    "editor.lineHighlightBackground": "#E6F2FF",
    "activityBar.background": "#F0F8FF",
    "activityBar.foreground": "#0A1420",
    "activityBar.border": "#c8dce8",
    "activityBar.activeBorder": "#0969DA",
    "sideBar.background": "#E6F2FF",
    "sideBar.foreground": "#1F2E3D",
    "sideBar.border": "#c8dce8",
    "editorGroup.border": "#c8dce8",
    "statusBar.background": "#0969DA",
    "statusBar.foreground": "#F0F8FF",
    "statusBar.border": "#c8dce8",
    "titleBar.activeBackground": "#F0F8FF",
    "titleBar.activeForeground": "#0A1420",
    "titleBar.inactiveBackground": "#F0F8FF",
    "titleBar.inactiveForeground": "#5B6B80",
    "titleBar.border": "#c8dce8",
    "panel.background": "#F0F8FF",
    "panel.border": "#c8dce8",
    "terminal.background": "#F0F8FF",
    "terminal.foreground": "#0A1420",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#0A1420",
    "tab.activeBorder": "#0969DA",
    "tab.inactiveBackground": "#E6F2FF",
    "tab.inactiveForeground": "#5B6B80",
    "tab.border": "#c8dce8",
    "editorLineNumber.foreground": "#5B6B80",
    "editorLineNumber.activeForeground": "#1F2E3D",
    "focusBorder": "#0969DA",
    "input.background": "#FFFFFF",
    "input.foreground": "#0A1420",
    "input.border": "#5B6B80",
    "inputOption.activeBorder": "#0969DA",
    "list.activeSelectionBackground": "#B3D7FF",
    "list.activeSelectionForeground": "#0A1420",
    "list.hoverBackground": "#D6E8FF",
    "list.hoverForeground": "#0A1420",
    "list.focusOutline": "#0969DA",
    "button.background": "#0969DA",
    "button.foreground": "#F0F8FF",
    "button.hoverBackground": "#0A58CA"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#4B5B70",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#7C3AED"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#0A1420"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#0E7490"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#6D28D9"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#0E7490"
      }
    }
  ]
}
