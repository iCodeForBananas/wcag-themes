    "dark theme",
    "light theme"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1E5A8E",
    "theme": "dark"
  },
  "license": "MIT",
  "scripts": {
    "build": "vsce package",
    "publish": "vsce publish",
    "dev": "vsce package && code --install-extension wcag-themes-1.0.0.vsix"
  },
